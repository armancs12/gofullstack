{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "strict": true,
      "jsx": "preserve",
      "importHelpers": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "baseUrl": "ui",
      "types": ["webpack-env"],
      "paths": {
        "@/*": ["./*"]
      },
      "lib": ["ESNext", "DOM", "DOM.Iterable", "ScriptHost"]
    },
    "include": ["**/*.ts", "**/*.tsx", "**/*.vue"],
}